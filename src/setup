
//main.sph

main{
	dimensions=2;
	write-times=true;
	write-times-interval-type=absolute-time;
	write-times-value=0.1;
}
time-control{
	startTime=0;
	endTime=1;
	timeStep=0.001;
}


//patches.sph

geometry.walls{
	elements={top-wall,bottom-wall};
}
geometry.inlets{
	elements={inlet1};
}
geometry.outlets{
	elements={outlet2};
}

inlets.inlet1{
	patch.type = inlet;
	source = STL;
	STL.name = "inlet.stl"
	boundary.type = fixedvelocity;
	value.type = uniform-vector;
	value = (0 10);
}
outlets.outlet1{
	patch.type = outlet;
	source = STL;
	STL.name = "outlet.stl"
	boundary.type = fixed-pressure;
	value.type = uniform-value;
	value = 0;	
}

walls.top-wall{
	patch.type = wall;
	source.type = STL;
	STL.name = "top-wall.stl";
	boundary.type = noSlipWall;
}
walls.bottom-wal{
	patch.type = wall;
	source.type = two_point_line;
	two_point_line.start = (0 0);
	two_point_line.end = (0 10);
	boundary.type = noSlipWall;
}


//mesher.sph

walls.top-wall{
	refinement.engine=ico-recursive;
	ico-recursive.levels=2;
}
walls.bottom-wall{
	refinement.engine=standart-full-wall;
}


